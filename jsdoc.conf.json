{
    "source": {
        "include": [
            "index.js",
            "auth.js",
            "passport.js",
            "models.js"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "readme": "README.md"
    },
    "plugins": ["plugins/markdown"]
}